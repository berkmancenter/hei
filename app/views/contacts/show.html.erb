<p id="notice"><%= notice %></p>

<p>
  <b>First Name:</b>
  <%= @contact.first_name %>
</p>

<p>
  <b>Last Name:</b>
  <%= @contact.last_name %>
</p>

<p>
  <b>Phone:</b>
  <%= @contact.phone %>
</p>

<p>
  <b>Email:</b>
  <%= @contact.email %>
</p>

<p>
  <b>Organizations:</b>
  <% @contact.organizations.each do |org| %>
	<%= org.name %><br />
  <% end %>
</p>


<%= link_to 'Edit', edit_contact_path(@contact) %> |
<%= link_to 'Back', contacts_path %>
